	"telemetry.enableCrashReporter": true,
	"telemetry.enableTelemetry": true,
	"csharp.format.enable": false,
	"editor.cursorBlinking": "phase",
	"git.autofetch": true,
	"javascript.referencesCodeLens.enabled": false,
